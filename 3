#1решение
import math


def main(a, b, m):
    total_sum = 0
    for j in range(1, m + 1):
        for c in range(1, b + 1):
            for k in range(1, a + 1):
                total_sum += (
                    math.atan(c) ** 7 +
                    80 * (57 + j ** 3 + 18 * k ** 2) ** 5 +
                    1
                )
    return total_sum

